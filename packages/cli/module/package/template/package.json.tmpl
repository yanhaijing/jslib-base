{
    "name": "<%=npmname%>",
    "version": "0.1.0",
    "description": "The best third party JS|TS library scaffold",
    "sideEffects": false,
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "exports": {
      "node": {
        "import": "./dist/index.mjs",
        "default": "./dist/index.js"
      },
      "default": {
        "import": "./dist/index.mjs",
        "default": "./dist/index.js"
      }
    },
    <% if (type === 'ts') { %>"types": "./types/index.d.ts",<% } %>
    "scripts": {
      "preinstall": "npx only-allow npm",
      "clean": "rimraf ./dist ./types",
      "start": "http-server -p 3000 -c-1",
      "test": "echo Error: missing tests",
      "release": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags"
    },
    "author": "<%=username%>",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "git://github.com/<%=username%>/<%=name%>.git"
    },
    "bugs": {
      "url": "https://github.com/<%=username%>/<%=name%>/issues"
    },
    "files": [
      "/dist",
      "/types",
      "*.d.ts"
    ],
    "engines": {
      "node": ">=14.0.0",
      "npm": ">=6.0.0"
    },
    "publishConfig": {
      "registry": "https://registry.npmjs.org",
      "access": "public"
    },
    "devDependencies": {
      "@js-lib/cli": "^<%=version%>",
      "cross-env": "^7.0.3",
      "rimraf": "^5.0.5",
      "http-server": "^14.1.1",
      "es5-shim": "^4.6.7"
    },
    "dependencies": {
    }
  }
  